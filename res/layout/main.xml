<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" android:background="#fff">
	
	<ImageView android:id="@+id/ImageView03" 
		android:src="@drawable/logo" 
		android:layout_width="300dip" 
		android:scaleType="fitXY" 
		android:layout_height="80dip" 
		android:paddingLeft="175dip"></ImageView>
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_vertical"
		android:orientation="vertical">
		<ImageView android:id="@+id/ImageView02" 
			android:src="@drawable/menu" 
			android:layout_height="30dip" 
			android:scaleType="fitXY" 
			android:paddingLeft="8dip" 
			android:layout_width="311dip"></ImageView>
		<LinearLayout android:id="@+id/LinearLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:paddingLeft="8dip">
		<ImageButton android:id="@+id/ViewMapButton" android:src="@drawable/selector" android:scaleType="fitXY" android:layout_width="150dip" android:layout_height="150dip" android:background="#fff" android:clickable="true" android:focusable="true">
		</ImageButton>
		<ImageButton android:id="@+id/PlanButton" android:src="@drawable/selector2" android:scaleType="fitXY" android:layout_height="150dip" android:clickable="true" android:focusable="true" android:background="#fff" android:layout_width="153dip" android:paddingLeft="3dip">
		</ImageButton>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:paddingLeft="8dip" android:paddingTop="3dip">
		<ImageButton android:id="@+id/PrefButton" android:src="@drawable/selector3" android:scaleType="fitXY" android:layout_width="150dip" android:layout_height="150dip" android:background="#fff" android:clickable="true" android:focusable="true">
		</ImageButton>
		<ImageButton android:id="@+id/AboutButton" android:src="@drawable/selector4" android:scaleType="fitXY" android:layout_height="150dip" android:clickable="true" android:focusable="true" android:background="#fff" android:layout_width="153dip" android:paddingLeft="3dip">
		</ImageButton>
		</LinearLayout>
	</LinearLayout>
	
	
	
	

</LinearLayout>
